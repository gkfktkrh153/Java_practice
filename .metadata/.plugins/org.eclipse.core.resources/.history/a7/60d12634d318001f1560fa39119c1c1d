package kosa.mission;

public class Mission {

	public static void main(String[] args) {
		//이름 나이 주소 변수에 각각
		String name = "지승용";
		int age = 26;
		String address = "서울특별시 용산구 동자동";
		
		System.out.println("이름 : " + name);
		System.out.println("나이 : " + age);
		System.out.println("주소 : " + address);

	}

}
